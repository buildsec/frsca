{
  "spec": {
    "template": {
      "spec": {
        "$setElementOrder/containers": [
          {
            "name": "kyverno"
          }
        ],
        "$setElementOrder/volumes": [
          {
            "name": "signing-secrets"
          },
          {
            "name": "oidc-info"
          },
          {
            "name": "ca-certs"
          }
        ],
        "containers": [
          {
            "$setElementOrder/volumeMounts": [
              {
                "mountPath": "/.sigstore"
              },
              {
                "mountPath": "/etc/ssl/certs/"
              }
            ],
            "name": "kyverno",
            "volumeMounts": [
              {
                "mountPath": "/etc/ssl/certs/",
                "name": "ca-certs",
                "readOnly": true
              }
            ]
          }
        ],
        "volumes": [
          {
            "configMap": {
              "name": "ca-certs"
            },
            "name": "ca-certs"
          }
        ]
      }
    }
  }
}
